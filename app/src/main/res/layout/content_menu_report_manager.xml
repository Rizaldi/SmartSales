<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".dashboard.MenuReport"
    tools:showIn="@layout/activity_menu_report_manager">
    <LinearLayout
        android:layout_below="@id/timeplace"
        android:orientation="vertical"
        android:layout_marginTop="10dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/efectivecall"
                android:background="@drawable/radius"
                android:padding="10dp"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:src="@drawable/telephone"
                    android:layout_width="45dp"
                    android:layout_height="45dp" />
                <TextView
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="Effective Calls"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="#fff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <View
                android:layout_width="10dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/psoreport"
                android:background="@drawable/radiust"
                android:padding="10dp"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:src="@drawable/propose_sales"
                    android:layout_width="45dp"
                    android:layout_height="45dp" />
                <TextView
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="Propose SO Report"
                    android:textSize="13dp"
                    android:textStyle="bold"
                    android:textColor="#fff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

        </LinearLayout>

        <!-- menu 2-->
        <LinearLayout
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/transaction"
                android:background="#ea6153"
                android:padding="10dp"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:src="@drawable/menu_payment"
                    android:layout_width="45dp"
                    android:layout_height="45dp" />
                <TextView
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="Transaction Report"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="#fff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <View
                android:layout_width="10dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/goodreturn"
                android:background="#e67e22"
                android:padding="10dp"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:src="@drawable/menu_goods"
                    android:layout_width="45dp"
                    android:layout_height="45dp" />
                <TextView
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="Goods Return Report"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="#fff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>

        <!-- menu 3-->

        <LinearLayout
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/refillvan"
                android:background="#2196F3"
                android:padding="10dp"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:src="@drawable/menu_refil"
                    android:layout_width="45dp"
                    android:layout_height="45dp" />
                <TextView
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="Refill Van Report"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="#fff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <View
                android:layout_width="10dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/customeraging"
                android:background="#8BC34A"
                android:padding="10dp"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:src="@drawable/customer_aging"
                    android:layout_width="45dp"
                    android:layout_height="45dp" />
                <TextView
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="Customer Aging Report"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="#fff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
<!--menu 4-->
        <LinearLayout
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/tracking"
                android:background="@drawable/blradius"
                android:padding="10dp"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:src="@drawable/menu_tracking"
                    android:layout_width="45dp"
                    android:layout_height="45dp" />
                <TextView
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="Tracking"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="#fff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <View
                android:layout_width="10dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/top10"
                android:background="@drawable/brradius"
                android:padding="10dp"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:src="@drawable/top10_report"
                    android:layout_width="45dp"
                    android:layout_height="45dp" />
                <TextView
                    android:text="Top 10 Customer"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="#fff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>
